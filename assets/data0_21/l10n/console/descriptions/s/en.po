#. Console/s
msgid "s_doppler"
msgstr "<value> Doppler effect will be used for sounds\n"
"^6Disable: s_doppler \"0\"\n"
"^6Enable: s_doppler \"1\"\n"
"^6Note: This command works only for OpenAL Sound Module."

#. Console/s
msgid "s_globalfocus"
msgstr "<0/1> Mutes the game when alt-tabbed.\n"
"^6Disable: s_globalfocus \"0\"\n"
"^6Enable: s_globalfocus \"1\""

#. Console/s
msgid "s_khz"
msgstr "<value> The sampling rate of the sounds in kilohertz.\n"
"^6Note: Anything above \"44\", which is 44.1kHz will cause the sound system to crash."

#. Console/s
msgid "s_mixahead"
msgstr "<value> The specified time waited before mixing sound samples."

#. Console/s
msgid "s_module"
msgstr "<0-2> Specifies the default sound module.\n"
"^6No Sound: s_module \"0\"\n"
"^6Qfusion: s_module \"1\"\n"
"^6OpenAL: s_module \"2\""

#. Console/s
msgid "s_module_fallback"
msgstr "<0-2> Specifies a fallback sound module incase s_module cannot be loaded.\n"
"^6No Sound: s_module_fallback \"0\"\n"
"^6Qfusion: s_module_fallback \"1\"\n"
"^6OpenAL: s_module_fallback \"2\""

#. Console/s
msgid "s_musicvolume"
msgstr "<0-1.0> Global music volume.\n"
"^6Disable: s_musicvolume \"0\""

#. Console/s
msgid "s_openAL_device"
msgstr "<value> Specifies the OpenAL device.\n"
"^6Example: s_openAL_device \"Rapture3D\""

#. Console/s
msgid "s_pseudoAcoustics"
msgstr "<0/1> High quality spatialisation.\n"
"^6Note: This command works only for Qfusion Sound Module."

#. Console/s
msgid "s_separationDelay"
msgstr "<value> Applies a delay to opposite ear of where the sound originates based on the angle.\n"
"^6Note: This command works only for Qfusion Sound Module."

#. Console/s
msgid "s_stereo2mono"
msgstr "<0/1> Mono sound.\n"
"^6Disable: s_stereo2mono \"0\"\n"
"^6Enable: s_stereo2mono \"1\""

#. Console/s
msgid "s_swapstereo"
msgstr "<0/1> Swap the left and right sound channel.\n"
"^6Disable: s_swapstereo \"0\"\n"
"^6Enable: s_swapstereo \"1\""

#. Console/s
msgid "s_volume"
msgstr "<0-1.0> Global volume of the game.\n"
"^6Disable: s_volume \"0\""

#. Console/s
msgid "s_wavonly"
msgstr "<0/1> Only .wav sound files will be played.\n"
"^6Disable: s_wavonly \"0\"\n"
"^6Enable: s_wavonly \"1\""

#. Console/s
msgid "s_devices"
msgstr "List of available OpenAL sound devices."

#. Console/s
msgid "s_restart"
msgstr "Restarts the sound engine."

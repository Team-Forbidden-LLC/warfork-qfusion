#. Console/tv
msgid "tv_autorecord"
msgstr "<0/1> Automatically records all TV demos.\n"
"^6Disable: tv_autorecord \"0\"\n"
"^6Enable: tv_autorecord \"1\""

#. Console/tv
msgid "tv_compresspackets"
msgstr "<0/1> Compresses TV network packets.\n"
"^6Disable: tv_compresspackets \"0\"\n"
"^6Enable: tv_compresspackets \"1\""

#. Console/tv
msgid "tv_floodprotection_messages"
msgstr "<value> The amount of messages a player is permitted to send before triggering flood protection.\n"
"^6Note: The amount of messages sent resets to \"0\" after the specified value of tv_floodprotection_seconds elapses."

#. Console/tv
msgid "tv_floodprotection_penalty"
msgstr "<value> No clue.\n"
"^6Note: No clue."

#. Console/tv
msgid "tv_floodprotection_seconds"
msgstr "<value> The amount of time before \"tv_floodprotection_messages\" resets."

#. Console/tv
msgid "tv_log_heartbeats"
msgstr "<0/1> Prints transmitted heartbeat messages to the TV server console.\n"
"^6Disable: tv_log_heartbeats \"0\"\n"
"^6Enable: tv_log_heartbeats \"1\""

